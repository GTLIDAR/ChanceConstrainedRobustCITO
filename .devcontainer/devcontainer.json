{
    "name": "pyDrake",
    "build": {"dockerfile": "Dockerfile"},
    "settings":{
        "git.path": "/usr/bin/git"
    },
    "forwardPorts": [3000, 7000],
    "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "smilerobotics.urdf"
    ],
    "runArgs": [
        "--net","host",
        "-e","DISPLAY=${env:DISPLAY}",
        "-v","/tmp/.X11-unix:/tmp/.X11-unix"
    ]
}
